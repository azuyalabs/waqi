{
  "name": "azuyalabs/waqi",
  "description": "Simple PHP Wrapper for the World Air Quality Index API",
  "keywords": [
    "air quality",
    "AQI",
    "API",
    "pollution",
    "pm2.5",
    "WAQI",
    "air"
  ],
  "type": "library",
  "homepage": "https://github.com/azuyalabs/waqi",
  "license": "MIT",
  "authors": [
    {
      "name": "Sacha Telgenhof",
      "email": "me@sachatelgenhof.com",
      "homepage": "https://www.sachatelgenhof.com",
      "role": "Maintainer"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "guzzlehttp/guzzle": "^7.4",
    "ext-json": "*"
  },
  "require-dev": {
    "mockery/mockery": "^1.3",
    "friendsofphp/php-cs-fixer": "^v3.5",
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan": "^1.4",
    "fakerphp/faker": "^1.15"
  },
  "autoload": {
    "psr-4": {
      "Azuyalabs\\WAQI\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Azuyalabs\\WAQI\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "format": "vendor/bin/php-cs-fixer fix",
    "check": "vendor/bin/phpstan analyse -l 5 src"
  }
}
