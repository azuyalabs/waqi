{
  "name": "azuyalabs/waqi",
  "description": "Simple PHP Wrapper for the World Air Quality Index API",
  "keywords": [
    "air quality",
    "AQI",
    "API",
    "pollution",
    "pm2.5",
    "WAQI",
    "air"
  ],
  "type": "library",
  "homepage": "https://github.com/azuyalabs/waqi",
  "license": "MIT",
  "authors": [
    {
      "name": "Sacha Telgenhof",
      "email": "me@sachatelgenhof.com",
      "homepage": "https://www.sachatelgenhof.com",
      "role": "Maintainer"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "guzzlehttp/guzzle": "^7.4",
    "ext-json": "*"
  },
  "require-dev": {
    "mockery/mockery": "^1.3",
    "friendsofphp/php-cs-fixer": "^v3.5",
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan": "^1.4",
    "fakerphp/faker": "^1.15",
    "infection/infection": "^0.26.1",
    "vimeo/psalm": "^4.18"
  },
  "autoload": {
    "psr-4": {
      "Azuyalabs\\WAQI\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Azuyalabs\\WAQI\\Test\\": "tests"
    }
  },
  "scripts": {
    "format": "vendor/bin/php-cs-fixer fix",
    "phpstan": "vendor/bin/phpstan analyse",
    "psalm": "vendor/bin/psalm --threads=2",
    "test": "vendor/bin/phpunit",
    "infection": "vendor/bin/infection run -j 2",
    "analyse": [
      "@phpstan",
      "@psalm",
      "@infection"
    ]
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "infection/extension-installer": true,
      "composer/package-versions-deprecated": true
    }
  }
}
